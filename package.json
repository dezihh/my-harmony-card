{
    "name": "my-harmony-card",
    "version": "v1.0.1",
    "description": "My Harmony Remote card",
    "keywords": [
        "home-assistant",
        "homeassistant",
        "hass",
        "automation",
        "lovelace",
        "custom-cards"
    ],
    "module": "my-harmony-card.js",
